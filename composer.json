{
  "name": "geeh/hactoros",
  "description": "PSR HTTP Message implementations forked from zendframework/zend-diactoros",
  "type": "library",
  "license": "MIT",
  "keywords": [
    "http",
    "psr",
    "psr-7"
  ],

  "require": {
    "hhvm": "~3.7.1",
    "psr/http-message": "~1.0"
  },
  "require-dev": {
    "phpunit/PHPUnit": "~4.6",
    "squizlabs/php_codesniffer": "^2.3.1"
  },
  "provide": {
    "psr/http-message-implementation": "~1.0.0"
  },
  "autoload": {
    "psr-4": {
      "GeeH\\Hactoros\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "ZendTest\\Diactoros\\": "test/"
    },
    "files": [
      "test/TestAsset/Functions.php",
      "test/TestAsset/SapiResponse.php"
    ]
  }
}
